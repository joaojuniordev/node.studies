#COPY AND REANAME: .end.jjrdev

#NODE_ENV=".env"
JJRDEV_NODE_ENV=".env.test"

#APP:
JJRDEV_APP_NAME="NODE.STUDIES"
JJRDEV_APP_LOCALHOST="http://localhost"
JJRDEV_APP_SECRET=""
JJRDEV_APP_UPLOAD_DIR=""
JJRDEV_APP_UPLOAD_LIB_ENABLED="false"
#EX_SCHEDULED 15min => https://www.freeformatter.com/cron-expression-generator-quartz.html
JJRDEV_APP_SCHEDULED="0*/15*?**"


#SERVER:
JJRDEV_SERVER_URL=""
