#EXPRESS:
JJRDEV_EXPRESS_PORT="3000"

#MIDDLEWARE:
JJRDEV_MW_REGISTER_IP="false"

#DBFAKE:
JJRDEV_DBFAKE_ACTIVATED_JSON="true"
